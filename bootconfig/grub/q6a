insmod part_gpt
insmod part_msdos
insmod all_video
terminal_input console
terminal_output console
if serial --unit=0 --speed=115200; then
	terminal_input --append console
	terminal_output --append console
fi
set timeout_style=menu
set timeout=0
set default=0
menuentry 'deepin Linux for Radxa Dragon Q6A' {
	echo 'Loading Kernel...'
	linux /boot/vmlinuz-6.15.7-q6a-radxa-615y panic=30 loglevel=8 rw rootwait=10 root=LABEL=root rootfstype=ext4
	echo 'Loading Initramfs...'
	initrd /boot/initrd.img-6.15.7-q6a-radxa-615y
	echo 'Booting...'
}
